            motion:layout_constraintStart_toEndOf="@+id/imageButton4"
            motion:layout_constraintTop_toTopOf="@+id/imageButton4"
            android:layout_marginLeft="24dp"
            android:layout_marginStart="24dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/imageFilterView"
            android:id="@+id/imageView8"
            motion:layout_constraintStart_toStartOf="@+id/view"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="28dp"
            android:layout_marginStart="28dp" />
        <Constraint
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteY="28dp"
