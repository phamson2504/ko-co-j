            android:layout_marginTop="4dp"
            motion:layout_constraintStart_toEndOf="@+id/imageView5"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp" />
        <Constraint
            android:layout_height="130dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_width="411dp"
            android:id="@+id/view"
            motion:layout_constraintTop_toBottomOf="@+id/view2"
            android:layout_marginTop="0dp" />
        <Constraint
            android:layout_height="588dp"
            android:layout_width="409dp"
            android:id="@+id/view2"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:layout_height="50dp"
            motion:layout_constraintTop_toTopOf="@+id/imageButton"
            android:layout_width="50dp"
            android:id="@+id/imageButton3"
            motion:layout_constraintStart_toEndOf="@+id/imageButton"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageFilterView"
            motion:layout_constraintTop_toTopOf="@+id/imageButton3"
            motion:layout_constraintStart_toEndOf="@+id/imageButton3"
